{
  "name": "ttt-threejs-tour",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@react-three/drei": "^7.3.1",
    "@react-three/fiber": "^7.0.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^4.20.1",
    "axios": "^0.27.2",
    "env-cmd": "^10.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "three": "^0.131.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "env-cmd -f ./src/environments/.env react-scripts --max_old_space_size=4096 start",
    "build": "env-cmd -f ./src/environments/.env react-scripts --max_old_space_size=4096 build",
    "build:dev": "env-cmd -f ./src/environments/.env.dev react-scripts build",
    "build:stage": "env-cmd -f ./src/environments/.env.stage react-scripts build",
    "build:prod": "env-cmd -f ./src/environments/.env.prod react-scripts build",
    "build:test": "env-cmd -f ./src/environments/.env.test react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
